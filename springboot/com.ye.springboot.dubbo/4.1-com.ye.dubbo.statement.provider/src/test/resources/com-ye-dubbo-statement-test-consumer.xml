<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="com.ye.dubbo.statement.test.consumer"  />

    <dubbo:consumer check="false" />

    <!-- 使用multicast广播注册中心暴露发现服务地址 -->
    <dubbo:registry protocol="zookeeper" address="127.0.0.1:2181"  group="local"/>
    <!-- 声明需要引用的服务接口 -->
    <dubbo:reference id="IBfOrderFileService" interface="com.ye.dubbo.statement.service.inf.bf.BfOrderFileService" version="1.0.0" validation="true" />

</beans>